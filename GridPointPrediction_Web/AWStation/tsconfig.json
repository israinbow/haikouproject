{
  "compilerOptions": {
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "es5",
    "module": "amd",
    "baseUrl": "./js",
    "paths": { "*": [ "*" ] }
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ],
  "compileOnSave": true
}
